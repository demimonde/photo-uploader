{
  "env": {
    "test-build": {
      "import": {
        "replacement": {
          "from": "^((../)+)src",
          "to": "$1build"
        }
      }
    },
    "build": {
      "import": {
        "replacement": {
          "from": "^(.+)/(.+).css$",
          "to": "$1/closure.css"
        }
      },
      "closure": {
        "cssRenamingPrefix": "pu-",
        "rootSelector": ".PhotoUploader"
      },
      "jsx": {
        "prop2class": true,
        "classNames": "example/bootstrap.json"
      }
    }
  }
}